<?xml version="1.0" encoding="ISO-8859-1" ?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/database.xsd">
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>
	<table>
		<name>*dbprefix*entauth</name>
		<declaration>
			<field>
				<name>ent</name>
				<type>integer</type>
				<notnull>true</notnull>
			</field>
			<field>
				<name>extid</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>uid</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<index>
				<name>entauth_ent_ext_id_idx</name>
				<primary>true</primary>
				<unique>true</unique>
				<field>
					<name>ent</name>
				</field>
				<field>
					<name>extid</name>
				</field>
			</index>
		</declaration>
	</table>
</database>
